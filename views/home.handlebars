<section>
    <div class="booking-search">
        <div class="book-form">
             {{#each messages}}
            <div class="errors">{{this}}</div>
            {{/each}} 
            <div class="form1">

                <h2>Beach House? Condo?<br> Your perfect location!</h2>


                <form class="form" method="post" action="/search">

                    <div class="form-label">
                        <label for="where"><b>Where : </b></label>

                        <input type="text" placeholder="Anywhere" name="Where">


                    </div>

                    <div class="form-label">
                        <label for="Arrive"><b>Arrive : </b></label>
                        <input type="date" placeholder="dd-mm-yyyy" name="Arrive">
                    </div>


                    <div class="form-label">
                        <label for="Depart"><b>Depart : </b></label>
                        <input type="date" placeholder="dd-mm-yyyy" name="Depart">
                    </div>

                    <div class="form-label">
                        <label for="guests"><b>Guests : </b></label>
                        <input type="text" placeholder="Guests" name="guests">
                    </div>

                    <input type="submit" class="search-button" value="Search">
                </form>
            </div>
        </div>
    </div>


    </div>
</section>
